#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules/.pnpm/eslint@8.23.1/node_modules/eslint/bin/node_modules:/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules/.pnpm/eslint@8.23.1/node_modules/eslint/node_modules:/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules/.pnpm/eslint@8.23.1/node_modules:/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules/.pnpm/node_modules:/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules:/Users/weizhang/Desktop/AbstractEr/node_modules:/Users/weizhang/Desktop/node_modules:/Users/weizhang/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules/.pnpm/eslint@8.23.1/node_modules/eslint/bin/node_modules:/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules/.pnpm/eslint@8.23.1/node_modules/eslint/node_modules:/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules/.pnpm/eslint@8.23.1/node_modules:/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules/.pnpm/node_modules:/Users/weizhang/Desktop/AbstractEr/vue3_frame/node_modules:/Users/weizhang/Desktop/AbstractEr/node_modules:/Users/weizhang/Desktop/node_modules:/Users/weizhang/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/eslint.js" "$@"
else
  exec node  "$basedir/../../bin/eslint.js" "$@"
fi
